    "material-icon-theme": "^5.24.0",
    "monaco-editor": "^0.50.0",
    "motion": "^12.20.4",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-resizable-panels": "^2.1.9",
    "react-terminal": "^1.4.5",
    "tailwindcss": "^4.1.11",
    "uuid": "^11.1.0",
    "zustand": "^4.5.7"
  },
  "devDependencies": {
    "@semantic-release/changelog": "^6.0.3",
